[user]
    name = 
    email = 
[color]
    ui = true
[core]
    autocrlf = input
    editor = vim
[alias]
    co = checkout
    st = status
    unstage = reset HEAD --
    #ll = log --oneline --graph --decorate --branches --tags --remotes
    ll = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold red)%h%C(reset) %C(bold green)(%ar)%C(reset) %C(bold blue)(%an)%C(reset) %C(white)%s%C(reset)%C(bold yellow)%d%C(reset)' --all
    ll1 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
    r = remote -v
    b = branch -a
